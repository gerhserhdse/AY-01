 int main(){
int  nums[]={2,7,11,15,11,7,2};
   int i=0;
  int sz=sizeof(nums)/sizeof(nums[0]);
  for(;i<sz;i++)
  { 
      int j=0;
      int count=0;
      for(;j<sz;j++){
    if(nums[i]==nums[j])
     {
     	
        count++;
     }
	 }
	  if(count==1)
     {
     	printf("单身狗是:>%d\n",nums[i]);
     	break;
      }
    
  }
return 0;
}
