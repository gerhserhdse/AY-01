package Idea代码.有用;

import java.util.Scanner;

public class 阶层 {
    public static void main(String[] args) {

        Scanner in=new Scanner(System.in);
        int n=in.nextInt();
        int f=1;
        int i=1;
        while(i<=n){
            f*=i;
            i++;
        }
        System.out.println("d!"+n+"  "+f);

    }
}
